{
  "name": "yourresume.io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server & npm run tailwindcss",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route",
    "tailwindcss": "npx tailwindcss -i ./public/style.css -o ./public/output.css --watch "
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^5.1.0",
    "bootstrap": "^3.4.1",
    "compression": "^1.7.4",
    "daisyui": "^2.51.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-map-react": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "openai": "^3.2.1",
    "pg": "^8.8.0",
    "react-multi-carousel": "^2.8.3",
    "react-slick": "^0.29.0",
    "sequelize": "^6.25.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.1.3",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "esm": "^3.2.25",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.11.1",
    "redux-logger": "^3.0.6",
    "supertest": "^6.3.0",
    "tailwindcss": "^3.3.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "browser": {
    "./client/index.js": "./client/index.js"
  }
}
